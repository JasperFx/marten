{
  "index_name": "marten_index",
  "start_urls": [
    "https://martendb.io"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".VPLink.link.VPNavBarMenuLink.active",
      "global": true
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "text": ".content p, .content li",
    "language": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true,
      "default_value": "en-US"
    }
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": ["sup"],
  "custom_settings": {
    "attributesForFaceting": ["language", "tags"]
  }
}
